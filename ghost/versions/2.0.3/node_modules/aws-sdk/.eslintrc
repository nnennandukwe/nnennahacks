{
  "env": {
    "browser": true,
    "node": true
  },
  "globals": {
    "Buffer": true,
    "escape": true
  },
  "rules": {
    "quotes": [2, "single"],
    "strict": 0,
    "curly": 0,
    "no-underscore-dangle": 0,
    "new-cap": 0,
    "dot-notation": 0,
    "no-use-before-define": 0,
    "no-require-in-service": 2
  }
}
